<div class="container">
	<h1>User List</h1>
	<table class="table">
		<thead>
			<tr>
				<th>Title</th>	
				<th>Name</th>
				<th>Email</th>
				<th></th>
				<th></th>
				<th></th>							
			</tr>
		</thead>
		<tbody>
			<% users.forEach(function(user){ %>
				<tr>
					<td><%= user.title %></td>
					<td><%= user.name %></td>
					<td><%= user.email %></td>
					<td><a class="btn btn-info" href="/user/show/<%= user.id %>">Show</a></td>
					<td><a class="btn btn-warning" href="/user/edit/<%= user.id %>">Edit</a></td>
					<td><a class="btn btn-danger" href="/user/destroy/<%= user.id %>">Delete</a></td>			
				</tr>
			<% }) %>
		</tbody>
	</table>
	<table class="table">
		<tr>
			<td>
				<a class="btn btn-primary" href="/user/new">Add User</a>  
				<a class="btn btn-primary" href="/">Go Home</a>
			</td>			
		</tr>		
	</table>
</div>